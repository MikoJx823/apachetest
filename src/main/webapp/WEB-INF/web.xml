<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Testing</servlet-name>
  	<display-name>Testing</display-name>
  	<description></description>
  	<servlet-class>com.project.servlet.Testing</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductListServlet</servlet-name>
  	<display-name>ProductListServlet</display-name>
  	<description></description>
  	<servlet-class>com.project.servlet.ProductListServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductDetailServlet</servlet-name>
  	<display-name>ProductDetailServlet</display-name>
  	<description></description>
  	<servlet-class>com.project.servlet.ProductDetailServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CartItemQtyServlet</servlet-name>
  	<display-name>CartItemQtyServlet</display-name>
  	<description></description>
  	<servlet-class>com.project.servlet.CartItemQtyServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CartServlet</servlet-name>
  	<display-name>CartServlet</display-name>
  	<description></description>
  	<servlet-class>com.project.servlet.CartServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CartSummaryServlet</servlet-name>
  	<display-name>CartSummaryServlet</display-name>
  	<description></description>
  	<servlet-class>com.project.servlet.CartSummaryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Testing</servlet-name>
  	<url-pattern>/Testing</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductListServlet</servlet-name>
  	<url-pattern>/products</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductDetailServlet</servlet-name>
  	<url-pattern>/productdetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CartItemQtyServlet</servlet-name>
  	<url-pattern>/cartitemqty</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CartServlet</servlet-name>
  	<url-pattern>/cart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CartSummaryServlet</servlet-name>
  	<url-pattern>/cartsummary</url-pattern>
  </servlet-mapping>
</web-app>
